// Program to print the GP.
import java.util.*;
public class findGP {
    public static void main(String[] args) {

        Scanner sc= new Scanner(System.in);
        int n= sc.nextInt();
        int a=3, r=4;

        for(int i=1; i<=n; i++) {
            System.out.println(a + " ");
            a *= r;
        }
    }
}
