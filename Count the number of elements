// count elements of an array
public class CountElements {
    public static void main(String[] args){
        int[] arr= {3,4,7,65,32,735,37235,72,357,325,73,257};

        int count=0;
        for(int i=0; i<arr.length; i++){
            if(arr[i]>i) count++;
           // System.out.println(count);

        }
        System.out.println(count);
    }
}
