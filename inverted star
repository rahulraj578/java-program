
import java.util.Scanner;
public class invertedStar {
    public static void main(String[] args) {
        Scanner sc= new Scanner(System.in);
        int n= sc.nextInt();
        System.out.println("enter the number of n");
        for(int i=1; i<=n; i++){
            for(int j=1; j<=n+1-i; j++){
                System.out.print("*");
            }
            System.out.println();
        }

    }
}
