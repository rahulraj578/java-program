
// program to print hte sum of an elements of an array
import java.util.*;
public class ArraySum {
    public static void main(String[] args){
        Scanner sc= new Scanner(System.in);
        System.out.println("Enter Array size");
        int n= sc.nextInt();
        int[] arr= new int[n];
        for(int i=0; i<n; i++){
            arr[i]=sc.nextInt();
        }
        int sum=0;
        for(int i=0; i<n; i++){
            sum+= arr[i];
        }
        System.out.println(sum);
    }
}
